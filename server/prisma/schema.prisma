datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String  @id
  name     String
  photoURL String?
  tweets   Tweet[]
}

model Tweet {
  id        String @id @default(uuid())
  userId    String
  text      String
  createdAt String
  user      User   @relation(fields: [userId], references: [id])
}
